{
  "trailingComma": "es5",
  "singleQuote": true,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": [
    "typescript",
    "jsx",
    "classProperties",
    "decorators-legacy"
  ],
  "importOrder": [
    "^react",
    "^next",
    "^@mui/(.*)$",
    "^(?![~/.])([^.]+)",
    "^[~/]",
    "^[.]",
    "."
  ]
}
